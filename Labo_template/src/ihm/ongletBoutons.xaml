<UserControl x:Class="Labo_template.ihm.ongletBoutons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Labo_template.ihm"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="10*"/>
        </Grid.ColumnDefinitions>
        <Button Name="Tab01Bouton01" Content="Clique moi" Margin="10,10,0,0" Height="20" Click="Tab01Bouton01_Click" VerticalAlignment="Top" />
        <Label Name="Tab01Label01" Content="Label" Grid.Column="1" Margin="10,10,0,0" Grid.RowSpan="2"/>
        <Button x:Name="Tab01Bouton02" Content="Vider" Margin="10,10,0,0" Click="Tab01Bouton02_Click" Grid.Row="1" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
