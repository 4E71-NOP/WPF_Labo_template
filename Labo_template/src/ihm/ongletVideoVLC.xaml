      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:vlc="clr-namespace:Vlc.DotNet.Wpf;assembly=Vlc.DotNet.Wpf"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">

    <Grid x:Name="gridTab13">
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="20*"/>
        </Grid.RowDefinitions>
        <!--<vlc:VlcControl x:Name="vlcPlayer" Grid.Row="0" />-->
        <Button x:Name="Tab13ButtonA01" Content="Fichier" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" MinWidth="128.2" Click="Tab13ButtonA01_Click"/>
        <Button x:Name="Tab13ButtonB01" Content="Lecture" Margin="10,35,0,0" VerticalAlignment="Top" Grid.Row="1" HorizontalAlignment="Left" MinWidth="128" Click="Tab13ButtonB01_Click"/>
        <Button x:Name="Tab13ButtonB02" Content="Stop" Margin="143,35,0,0" VerticalAlignment="Top" Grid.Row="1" MinWidth="128" HorizontalAlignment="Left" Click="Tab13ButtonB02_Click"/>
    </Grid>
